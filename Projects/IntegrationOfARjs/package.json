{
  "name": "my-project",
  "version": "1.0.0",
  "scripts": {
    "start": "vite --host",
    "build:dev": "tsc && vite build && npm run copy-files",
    "build:production": "npm run build:dev && npm run pack-gltf",
    "serve": "vite preview",
    "copy-files": "copy-files-from-to",
    "pack-gltf": "npm run pack-gltf --prefix node_modules/@needle-tools/engine"
  },
  "dependencies": {
    "@ar-js-org/ar.js": "^3.4.1",
    "@needle-tools/engine": "file:../../Library/PackageCache/com.needle.engine@2.41.0-pre/package~",
    "three": "file:../../Library/PackageCache/com.needle.engine@2.41.0-pre/package~/node_modules/three"
  },
  "devDependencies": {
    "@gltf-transform/cli": "^2.1.7",
    "@gltf-transform/functions": "^2.1.7",
    "@needle-tools/needle-component-compiler": "^1.0.0",
    "@types/three": "^0.134.0",
    "ansi-styles": "^6.1.0",
    "copy-files-from-to": "^3.2.2",
    "draco3dgltf": "^1.5.0",
    "glob": "^7.2.0",
    "rollup-plugin-visualizer": "^5.5.2",
    "typescript": "^4.3.2",
    "vite": "2.6.4",
    "vite-plugin-compression": "^0.3.6"
  },
  "copyFiles": [
    {
      "from": "assets/**/*.*",
      "to": "dist/assets/"
    },
    {
      "from": "node_modules/@needle-tools/engine/include/**/*.*",
      "to": "dist/include/"
    },
    {
      "from": "node_modules/@needle-tools/engine/include/**/*.*",
      "to": "./include/"
    },
    {
      "from": "include/**/*.*",
      "to": "dist/include/"
    }
  ]
}
